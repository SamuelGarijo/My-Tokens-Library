{
  "global/primitives": {
    "scale": {
      "fontsize": {
        "value": "16",
        "type": "fontSizes"
      }
    },
    "base": {
      "value": "{scale.fontsize} * 0.5",
      "type": "sizing"
    },
    "xss": {
      "value": "{base} * 0.25",
      "type": "sizing"
    },
    "xs": {
      "value": "{base} * 0.5",
      "type": "sizing"
    },
    "sm": {
      "value": "{base} * 1",
      "type": "sizing"
    },
    "md": {
      "value": "{base} * 2",
      "type": "sizing",
      "description": "This is the middle way, the 1rem value and the fontsize base."
    },
    "lg": {
      "value": "{base} * 3",
      "type": "sizing"
    },
    "xl": {
      "value": "{base} * 5",
      "type": "sizing"
    },
    "xxl": {
      "value": "{base} * 6",
      "type": "sizing"
    },
    "xxxl": {
      "value": "{base} * 7",
      "type": "sizing"
    }
  },
  "global/components": {
    "maxwidth": {
      "container": {
        "web": {
          "value": "{xxxl} * 20",
          "type": "sizing"
        }
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "global/primitives",
      "global/components"
    ]
  }
}