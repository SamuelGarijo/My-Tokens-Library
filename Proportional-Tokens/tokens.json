{
  "global/primitives": {
    "scale": {
      "fontsize": {
        "value": "16",
        "type": "fontSizes"
      }
    },
    "base": {
      "value": "{scale.fontsize} * 0.5",
      "type": "sizing"
    },
    "xss": {
      "value": "{base} * 0.25",
      "type": "sizing"
    },
    "xs": {
      "value": "{base} * 0.5",
      "type": "sizing"
    },
    "sm": {
      "value": "{base} * 1",
      "type": "sizing"
    },
    "md": {
      "value": "{base} * 2",
      "type": "sizing",
      "description": "This is the middle way, the 1rem value and the fontsize base."
    },
    "lg": {
      "value": "{base} * 3",
      "type": "sizing"
    },
    "xl": {
      "value": "{base} * 5",
      "type": "sizing"
    },
    "xxl": {
      "value": "{base} * 6",
      "type": "sizing"
    },
    "xxxl": {
      "value": "{base} * 7",
      "type": "sizing"
    }
  },
  "global/components": {
    "container": {
      "max-width": {
        "value": "{xxxl} * 20",
        "type": "sizing"
      }
    }
  },
  "global/components/dsk": {
    "container": {
      "max-width": {
        "value": "{xxxl} * 20",
        "type": "sizing"
      }
    },
    "section": {
      "padding": {
        "vertical": {
          "value": "{xl} * 1.5",
          "type": "spacing"
        },
        "horizontal": {
          "value": "{xl}",
          "type": "spacing"
        }
      }
    },
    "navbar": {
      "padding": {
        "vertical": {
          "value": "{md}",
          "type": "spacing"
        },
        "horizontal": {
          "value": "{section.padding.horizontal}",
          "type": "spacing"
        },
        "gap": {
          "value": "{xl}",
          "type": "spacing"
        }
      },
      "row-links": {
        "padding": {
          "gap": {
            "value": "{lg}",
            "type": "spacing"
          }
        }
      }
    }
  },
  "global/components/mob": {
    "container": {
      "max-width": {
        "value": "{xxxl} * 8",
        "type": "sizing"
      }
    },
    "section": {
      "padding": {
        "vertical": {
          "value": "50",
          "type": "spacing"
        }
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "global/primitives",
      "global/components",
      "global/components/dsk",
      "global/components/mob"
    ]
  }
}